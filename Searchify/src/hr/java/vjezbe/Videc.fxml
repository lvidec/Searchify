<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.java.vjezbe.VidecController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Artikli">
            <items>
                  <Menu mnemonicParsing="false" text="Automobili">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#prikaziPretraguAutomobila" text="Pretraga" />
                        <MenuItem mnemonicParsing="false" onAction="#prikaziUnosAutomobila" text="Unos" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Stanovi">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#prikaziPretraguStanova" text="Pretraga" />
                        <MenuItem mnemonicParsing="false" onAction="#prikaziUnosStanova" text="Unos" />
                     </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Usluge">
                     <items>
                    <MenuItem mnemonicParsing="false" onAction="#prikaziPretraguUsluga" text="Pretraga" />
                        <MenuItem mnemonicParsing="false" onAction="#prikaziUnosUsluga" text="Unos" />
                     </items>
                  </Menu>
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Korisnici">
               <items>
                  <Menu mnemonicParsing="false" onAction="#prikaziPretraguPrivatnih" text="Privatni">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Pretraga" />
                        <MenuItem mnemonicParsing="false" onAction="#prikaziUnosPrivatnih" text="Unos" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" onAction="#prikaziPretraguPoslovnih" text="Poslovni">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Pretraga" />
                        <MenuItem mnemonicParsing="false" onAction="#prikaziUnosPoslovnih" text="Unos" />
                    </items>
                  </Menu>
               </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Prodaja">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#prikaziPretraguProdaje" text="Pretraga" />
                  <MenuItem mnemonicParsing="false" onAction="#prikaziUnosProdaje" text="Unos" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
